<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
	    <Console name="STDOUT" target="SYSTEM_OUT">
	    	<PatternLayout pattern="%-4r %-5p (%F:%L) - %m%n"/>
	    </Console>
    </Appenders>
	<Loggers>     
	    <Root level="fatal">
	        <AppenderRef ref="STDOUT" />
	    </Root>
    </Loggers>
</Configuration>
