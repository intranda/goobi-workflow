<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib
    version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd">
    <namespace>http://goobi.io/media</namespace>
    <tag>
        <tag-name>video</tag-name>
        <source>media/video.xhtml</source>
        <attribute>
            <description>The URL of the video file.</description>
            <name>source</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>audio</tag-name>
        <source>media/audio.xhtml</source>
        <attribute>
            <description>The URL of the audio file.</description>
            <name>source</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
</facelet-taglib>